{
  "manifest_version": 3,
  "name": "Chai-Themed Timer",
  "version": "1.0",
  "description": "A chai-inspired countdown timer for your Edge home page.",
  "permissions": ["storage"],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
